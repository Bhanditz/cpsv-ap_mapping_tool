SELECT
AL4.ENTITY,
AL4.ENTITY_NAME,
AL2.PORTFOLIO,
AL2.PORTFOLIO_NAME,
AL3.SECURITY_MAIN_ID,
AL3.SECURITY_MAIN_ID_FULL,
AL3.SECURITY_ISIN,
AL3.SECURITY_TICKER,
AL3.SECURITY_LONG_NAME,
AL3.SECURITY_CLV,
AL3.SECURITY_TVL,
AL3.SECURITY_GROUP,
AL3.SECURITY_TYPE,
AL3.INV_DASHBOARD_CLASS_TOP,
AL1.ACCOUNTING_STANDARD,
AL1.OPERATION_REFERENCE,
AL1.ACCOUNTING_DATE,
AL1.TRADE_DATE,
AL1.OPERATION_TYPE,
AL1.OPERATION_SECTION,
AL1.OPERATION_CLASS,
AL1.OPERATION_CLASS_LABEL,
AL1.ACCOUNT_CLASS,
AL1.AMOUNT_CLASS,
AL1.AMOUNT_CLASS_LABEL,
AL1.AMOUNT_CATEGORY,
AL1.AMOUNT_CATEGORY_LABEL,
AL1.TMN,
AL1.ACCOUNT,
AL1.ACCOUNT_LABEL,
AL1.AMOUNT_PC,
Decode(AL4.ENTITY,
'VIE',
'Ethias
SA',
'NVI',
'Ethias
SA',
'Other'),
AL5.PS_TOP_PK,
AL1.ACCOUNT,
AL6.USR_SAISI,
AL6.USR_ENVOYE,
AL6.USR_FRONT,
AL6.USR_ACCEPTE,
AL6.USR_MARCHE,
AL6.USR_VALIDEFO,
AL6.USR_ENCOURS,
AL6.USR_COMPLET,
AL6.USR_VERIFIEE,
AL6.USR_TERMINEE,
AL6.USR_INTERFAC,
AL6.USR_ANNULEE,
AL6.USR_EXTOURNE
FROM
DWHEN_PRD.V_ETHIAS_ACCOUNTING_ENTRIES
AL1,
DWHEN_PRD.V_ETH_PORTFOLIO
AL2,
DWHEN_PRD.V_ETH_SECURITY_DESCRIPTIVE
AL3,
DWHEN_PRD.V_ETH_ENTITY
AL4,
DWHEN_PRD.DIM_TOP
AL5,
DWHEN_PRD.DIM_BOR_OPE
AL6
WHERE
(AL4.ENT_KEY=AL1.ENT_KEY
AND
AL2.PTF_KEY=AL1.PTF_KEY
AND
AL3.INS_FIN_KEY=AL1.SECURITY_KEY
AND
AL5.TOP=AL1.OPERATION_TYPE
AND
AL6.OPE_STR_REFEXT=AL1.OPERATION_REFERENCE)

AND
((AL3.CURRENT_VERSION_SWI='Y'
AND
AL1.ACCOUNTING_STANDARD='BGAAP'
AND
AL1.ACCOUNTING_DATE
BETWEEN
'
1-01-2016
00:00:00'
AND
'
30-06-2016
00:00:00'
AND
AL4.CURRENT_VERSION_SWI='Y'
AND
AL2.CURRENT_VERSION_SWI='Y'
AND
AL1.OPERATION_TYPE='CHPLALAC'
AND
AL6.OPE_STR_REFXTR
IS
NULL
AND
AL2.ENTITY
IN
('AAM',
'NVI',
'VIE')))
